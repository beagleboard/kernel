+/*============================================================================*/
+/* tmbslTDA9989PktSetGeneralCntrl                                              */
+/*============================================================================*/
+
+tmErrorCode_t
+tmbslTDA9989PktSetGeneralCntrl
+(
+    tmUnitSelect_t      txUnit,
+    tmbslHdmiTxaMute_t  *paMute,
+    Bool                bEnable
+)
+{
