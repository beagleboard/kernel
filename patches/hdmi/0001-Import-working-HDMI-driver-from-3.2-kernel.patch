+    pSWVersion->compatibilityNr = VERSION_COMPATIBILITY;
+    pSWVersion->majorVersionNr = VERSION_MAJOR;
+    pSWVersion->minorVersionNr = VERSION_MINOR;
+
+    return TM_OK;
