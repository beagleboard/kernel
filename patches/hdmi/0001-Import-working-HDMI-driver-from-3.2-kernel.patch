+   case CEC_DEVICE_TYPE_AUDIO_DEVICE: 
+      switch (la) {
+      case CEC_LOGICAL_ADDRESS_AUDIO_SYSTEM:
+         return CEC_LOGICAL_ADDRESS_UNREGISTRED_BROADCAST;
+      default:
