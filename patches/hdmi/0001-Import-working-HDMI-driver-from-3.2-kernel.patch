+    case HDMITX_FEATURE_HW_AUDIO_OBA:
+      *pFeatureSupported = True;
+      break;
+    case HDMITX_FEATURE_HW_AUDIO_DST:
+      *pFeatureSupported = False;
+      break;
+    case HDMITX_FEATURE_HW_AUDIO_HBR:
+      *pFeatureSupported = False;
+      break;
+    case HDMITX_FEATURE_HW_HDMI_1_1:
+      *pFeatureSupported = True;
