+tmErrorCode_t tmdlHdmiTxGetSWVersion
+(
+    tmSWVersion_t   *pSWVersion
+)
+{
+    /* Check if SWVersion pointer is Null */
+    RETIF(pSWVersion == Null, TMDL_ERR_DLHDMITX_INCONSISTENT_PARAMS)
