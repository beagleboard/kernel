+/* -------------------------------------------------------------------------- */
+
+/*------------------ CTYP_BUS_NOTYPE dependent layer definitions -------------*/
+#define CID_LAYER_UDS      (0x7U << CID_LAYER_BITSHIFT)  /* USB Device Stack   */
