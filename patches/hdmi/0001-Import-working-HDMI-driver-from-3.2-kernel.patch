+   else {
+      /* block */
+      errCode = blockread_reg(GetThisI2cClient(), \
+			      pSysArgs->firstRegister, \
+			      pSysArgs->lenData, \
+			      pSysArgs->pData);
+   }
+   
+   /* restore default client address */
+   client->addr=client_main_addr;
+
+    return errCode;
