+  /* Save Datas of the CEC message send */
+  gtmdlHdmiCecDriverSaveMessage.AddressByte = I2c_Buffer[2];
+  gtmdlHdmiCecDriverSaveMessage.MessageTypePolling = 0;
+  gtmdlHdmiCecDriverSaveMessage.Opcode = I2c_Buffer[3];
+
+  return(TM_OK);
+}
