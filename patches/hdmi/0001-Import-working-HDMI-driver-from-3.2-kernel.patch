+
+*/
+//==========================================================================
+tmErrorCode_t tmdlHdmiCecTunerDeviceStatusAnalogue
+(
+   tmInstance_t                       Instance,
+   UInt8                              ReceiverLogicalAddress,
+   tmdlHdmiCECRecordingFlag_t         RecordingFlag,
+   tmdlHdmiCECTunerDisplayInfo_t      TunerDisplayInfo,
