 	{
 		.code                   = KEY_DOWN,
 		.gpio                   = GPIO_TO_PIN(3, 16),
-		.active_low             = false,
+		.active_low             = true,
 		.desc                   = "down",
 		.type                   = EV_KEY,
 		.wakeup                 = 1,
